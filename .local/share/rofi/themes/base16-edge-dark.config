! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #88909f, #88909f, #262729

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #88909f, #b7bec9, #88909f, #88909f, #3e4249
rofi.color-active:  #88909f, #73b3e7, #88909f, #88909f, #73b3e7
rofi.color-urgent:  #88909f, #e77171, #88909f, #88909f, #e77171

! Set the desired separator style
rofi.separator-style: solid
