! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #e7e6df, #e7e6df, #f4f3ec

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #e7e6df, #5f5e4e, #e7e6df, #e7e6df, #22221b
rofi.color-active:  #e7e6df, #36a166, #e7e6df, #e7e6df, #36a166
rofi.color-urgent:  #e7e6df, #ba6236, #e7e6df, #e7e6df, #ba6236

! Set the desired separator style
rofi.separator-style: solid
