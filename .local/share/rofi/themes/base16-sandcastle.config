! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #2c323b, #2c323b, #282c34

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #2c323b, #a89984, #2c323b, #2c323b, #fdf4c1
rofi.color-active:  #2c323b, #83a598, #2c323b, #2c323b, #83a598
rofi.color-urgent:  #2c323b, #83a598, #2c323b, #2c323b, #83a598

! Set the desired separator style
rofi.separator-style: solid
