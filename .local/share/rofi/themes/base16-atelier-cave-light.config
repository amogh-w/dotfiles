! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #e2dfe7, #e2dfe7, #efecf4

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #e2dfe7, #585260, #e2dfe7, #e2dfe7, #19171c
rofi.color-active:  #e2dfe7, #576ddb, #e2dfe7, #e2dfe7, #576ddb
rofi.color-urgent:  #e2dfe7, #be4678, #e2dfe7, #e2dfe7, #be4678

! Set the desired separator style
rofi.separator-style: solid
