! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #dfe7e2, #dfe7e2, #ecf4ee

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #dfe7e2, #526057, #dfe7e2, #dfe7e2, #171c19
rofi.color-active:  #dfe7e2, #478c90, #dfe7e2, #dfe7e2, #478c90
rofi.color-urgent:  #dfe7e2, #b16139, #dfe7e2, #dfe7e2, #b16139

! Set the desired separator style
rofi.separator-style: solid
