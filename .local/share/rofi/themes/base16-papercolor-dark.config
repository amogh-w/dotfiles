! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #af005f, #af005f, #1c1c1c

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #af005f, #808080, #af005f, #af005f, #d0d0d0
rofi.color-active:  #af005f, #ff5faf, #af005f, #af005f, #ff5faf
rofi.color-urgent:  #af005f, #585858, #af005f, #af005f, #585858

! Set the desired separator style
rofi.separator-style: solid
