! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #392551, #392551, #feffff

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #392551, #5a496e, #392551, #392551, #190134
rofi.color-active:  #392551, #47f9f5, #392551, #392551, #47f9f5
rofi.color-urgent:  #392551, #27d9d5, #392551, #392551, #27d9d5

! Set the desired separator style
rofi.separator-style: solid
