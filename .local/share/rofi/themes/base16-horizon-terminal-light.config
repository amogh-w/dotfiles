! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #fadad1, #fadad1, #fdf0ed

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #fadad1, #403c3d, #fadad1, #fadad1, #201c1d
rofi.color-active:  #fadad1, #26bbd9, #fadad1, #fadad1, #26bbd9
rofi.color-urgent:  #fadad1, #e95678, #fadad1, #fadad1, #e95678

! Set the desired separator style
rofi.separator-style: solid
